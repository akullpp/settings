	"eslintConfig": {
		"env": {
			"browser": true,
			"es6": true
		},
		"parser": "babel-eslint",
		"settings": {
			"import/resolver": {
				"node": {
					"paths": [
						"src"
					]
				}
			}
		},
		"extends": [
			"airbnb",
			"airbnb/hooks",
			"plugin:prettier/recommended",
			"prettier/react"
		],
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			},
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "error",
			"react/jsx-filename-extension": [
				"error",
				{
					"extensions": [
						".js"
					]
				}
			],
			"react/jsx-props-no-spreading": "off",
			"react/prop-types": "off",
			"import/prefer-default-export": "off"
		}
	},
